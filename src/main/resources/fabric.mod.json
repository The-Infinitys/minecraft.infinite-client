{
	"schemaVersion": 1,
	"id": "infinite",
	"version": "${version}",
	"name": "Infinite",
	"description": "This is a hacked client for anti anti-cheat",
	"authors": [
		"The Infinity's"
	],
	"contact": {
		"homepage": "https://the-infinitys.f5.si/minecraft/",
		"sources": "https://github.com/The-Infinitys/minecraft.infinite-client"
	},
	"license": "CC0-1.0",
	"icon": "assets/infinite/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			{
				"value": "org.theinfinitys.InfiniteClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "org.theinfinitys.InfiniteDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		{
			"config": "infinite.client.mixins.json",
			"environment": "client"
		}
	],
  "accessWidener": "infinite.accesswidener",
  "depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.21.8",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}