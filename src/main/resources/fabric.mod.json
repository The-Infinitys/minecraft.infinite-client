{
  "schemaVersion": 1,
  "id": "infinite",
  "version": "${version}",
  "name": "Infinite Client",
  "description": "Infinite Ultimate Utilities",
  "authors": [
    "The Infinity's"
  ],
  "contact": {
    "homepage": "https://the-infinitys.f5.si/minecraft/",
    "sources": "https://github.com/The-Infinitys/minecraft.infinite-client"
  },
  "license": "MIT",
  "icon": "assets/infinite/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "value": "org.infinite.InfiniteClient",
        "adapter": "kotlin"
      }
    ],
    "fabric-datagen": [
      {
        "value": "org.infinite.InfiniteDataGenerator",
        "adapter": "kotlin"
      }
    ],
    "modmenu": [
      "org.infinite.libs.mods.modmenu.InfiniteModMenuApi"
    ]
  },
  "mixins": [
    {
      "config": "infinite.client.mixins.json",
      "environment": "client"
    }
  ],
  "accessWidener": "infinite.accesswidener",
  "depends": {
    "fabricloader": ">=0.16",
    "minecraft": "~1.21.8",
    "java": ">=21",
    "fabric-api": "*",
    "fabric-language-kotlin": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/Xc3p52yDgK",
        "modmenu.issues": "https://github.com/The-Infinitys/minecraft.infinite-client/issues"
      },
      "badges": [
        "client"
      ],
      "configuredScreen": "org.infinite.gui.screen.GlobalSettingsScreen::create",
      "update_checker": true
    }
  },
  "suggests": {
    "baritone": "~1.21.10",
    "modmenu": "~16"
  }
}